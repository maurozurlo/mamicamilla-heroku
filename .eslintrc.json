{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "off",
        "no-undefined": "warn",
        "no-prototype-builtins": "warn",
        "no-restricted-globals": "off",
        "no-constant-condition": "off",
        "no-fallthrough": "off"
    },
    "globals": {
        "publicDir":"readonly",
        "viewsDir": "readonly"
    }
}
